{
    "name": "sendto-terminal",
    "description": "",
    "license" : "MIT",
    "version": "0.0.3",
    "publisher": "frankbehrens",
    "repository": {
        "type": "git",
        "url": "https://github.com/fbehrens/vscode-sendto-terminal.git"
    },
     "contributors":
    [   { "name": "Frank Behrens"
        , "url": "https://twitter.com/bluse1995"
        }
    ],
      "engines": {
        "vscode": "^1.5.3"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:sendtoTerminal.lineOrSelection",
        "onCommand:sendtoTerminal.lastCommand",
        "onCommand:sendtoTerminal.hello"
    ],
    "main": "./release/main",
    "contributes": {
        "keybindings": [
            {
                "command": "sendtoTerminal.hello",
                "key": "shift+ctrl+h"
            },
            {
                "command": "sendtoTerminal.lastCommand",
                "key": "shift+ctrl+l"
            },
            {
                "command": "sendtoTerminal.lineOrSelection",
                "key": "alt+enter"
            }
        ],
        "commands": [
            {
                "command": "sendtoTerminal.lineOrSelection",
                "title": "SentToTerminal: paste Selection"
            },
            {
                "command": "sendtoTerminal.lastCommand",
                "title": "SentToTerminal: redo last command"
            },
            {
                "command": "sendtoTerminal.hello",
                "title": "SentToTerminal: hello"
            }
        ]
    },
    "scripts": {
        "build": "fable src/main.fsproj --outDir ../release -m commonjs --sourceMaps --verbose"
    },
    "devDependencies": {
        "fable-compiler": "^0.6.9"
    },
    "dependencies": {
        "fable-core": "^0.6.7"
    }
}
