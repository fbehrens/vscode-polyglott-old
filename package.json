{
  "name": "fabledemo",
  "displayName": "fabledemo",
  "description": "",
  "version": "0.0.1",
  "publisher": "Chris",
  "engines": {
    "vscode": "^1.5.3"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.sayHello"
  ],
  "main": "./out/extension",
  "keybindings": [
    {
      "command": "extension.sayHello",
      "key": "shift+ctrl+x"
    } 
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.sayHello",
        "title": "Hello World"
      }
    ]
  },

  "scripts": {
    "build": "fable demo.fsproj --outDir ./out -m commonjs --sourceMaps --verbose"
  },
  "devDependencies": {
    "fable-compiler": "^0.6.9"
  },
  "dependencies": {
    "fable-core": "^0.6.7"
  }
}
